	width:100%;
	text-align:center;
}

/*left*/
body {
	margin: 0;
	padding: 0;
}

li {
	line-height: 20px;
}

.nav-list {
	list-style: outside none none;
	margin: 0;
	padding: 0;
}

.nav-list > li {
	border-image: none;
	display: block;
	margin: 0;
	padding: 0;
	position: relation;
}

.nav-list > li > a {
	background-color: #f9f9f9;
	color: #585858;
	display: block;
	font-size: 13px;
	height: 38px;
	line-height: 36px;
	padding: 0 16px 0 7px;
	text-decoration: none;
	text-shadow: none !important;
	margin: 0;
	border-bottom: #999999 solid 1px;
}

.nav-list > li > a:hover::before {
	background-color: #3382af;
	bottom: 0;
	content: "";
	display: block;
	left: 0;
	max-width: 3px;
	overflow: hidden;
	position: absolute;
	top: -1px;
	width: 3px;
}

.nav-list>li>a:hover {
	backgorund-color: #fff;
	color: #1963aa;
}

.nav-list li ul.submenu {
	list-style: outside none none;
	padding: 0;
	margin: 0;
}

.nav-list li ul li {
	border-image: none;
	display: block;
	margin: 0;
	padding: 0;
	position: relation;
	border-bottom: #999999 solid 1px;
}

.nav-list li ul li a {
	background-color: #fcfcfc;
	color: #585858;
	display: block;
	font-size: 13px;
	height: 38px;
	line-height: 36px;
	padding: 0 16px 0 27px;
	text-decoration: none;
	text-shadow: none !important;
	margin: 0;
}

