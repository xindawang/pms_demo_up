@CHARSET "UTF-8";
ul,ol,li{
	margin:0;
	padding:0;
}
li{
	list-style:none;
}

ul.breadcrumb {
	background: transparent none repeat scroll 0 0;
	border-radius: 2px;
	font-size: 12px;
	padding-left:10px;
	list-style-type:none;
	display:inline-block;
	*display:inline;
	line-height:36px;
	height:36px;
	padding-top:0;
	padding-bottom:0;
	margin-bottom:0;
}

ul.breadcrumb > li {	
	display:inline-block;
	vertical-align:top;
	margin:0;
	padding:0;
	line-height:36px;
	height:36px;
}

.content-header>.breadcrumb>li>a {
	color: #444;
	text-decoration: none;
}

.content-header > .breadcrumb > li+li:before {
	content: "> ";
}

.breadcrumb>.active {
	color: #999;
}

body {
	margin-top: 0;
}

html {
	margin: 0;
	padding: 0;
}

.body {
	margin:5px;
	overflow:scroll;
	height:98%;
}

.content-header {
	background: #ebebeb;
	border-bottom: #cccccc solid 1px;
	height: 38px;
	line-height:38px;	
