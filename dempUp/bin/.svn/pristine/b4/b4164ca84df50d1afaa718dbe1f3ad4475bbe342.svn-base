.thickframe {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000000;
    width: 100%;
    height: 100%;
    background: #000;
    border: 0;
    filter: alpha(opacity=0);
    opacity: 0;
}
.thickdiv {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000001;
    width: 100%;
    height: 100%;
    background: #000;
    border: 0;
    filter: alpha(opacity=15);
    opacity: .15;
}
.thickbox {
    position: absolute;
    z-index: 10000002;
    overflow: hidden;
    padding: 0;
    border: 4px solid rgba(0,0,0,.1);
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
}
.thicktitle {
    height: 31px;
    padding: 0 10px;
    background: #f5f5f5;
    line-height: 31px;
    font-family: arial,"\5b8b\4f53";
    font-size: 14px;
    font-weight: 700;
    color: #333;
    border: 0;
}

.thickcon {
    background: #fff;
    overflow: auto;
    padding: 10px;
}
.m {
    margin-bottom: 10px;
    overflow: hidden;
    zoom: 1;
}
.mc {
    overflow: hidden;
    zoom: 1;
}
.icon-box {
    position: relative;
    _height: 48px;
    _overflow: visible;
    zoom: 1;
}
.icon-box .m-icon {
    display: inline-block;
    position: absolute;
    top: 0;
    left: 0;
    height: 0;
    width: 48px;
    height: 48px;
    background: url(/images/icon48.png) no-repeat;
}
.icon-box .warn-icon {
    background-position: -96px 0;
}

.icon-box .item-fore {
    margin-left: 58px;
}
.icon-box {
    position: relative;
    _height: 48px;
    _overflow: visible;
}
.tip-box .item-fore {
    overflow: hidden;
}
.ftx04, .ftx-04 {
    color: #ff8a15;
    margin: 0;
    padding: 0;
}
.tip-box h3 {
    font-family: "microsoft yahei";
    font-size: 16px;
    line-height: 30px;
}
.ftx03, .ftx-03 {
    color: #999;
    margin: 0;
    padding: 0;
}
.tip-box .op-btns {
    margin-top: 20px;
}
.btn-9:link, .btn-9:visited, .btn-10:link, .btn-10:visited, .btn-11:link, .btn-11:visited, .btn-12:link, .btn-12:visited {
    color: #323333;
    text-decoration: none;
}
.btn-9:hover, .btn-10:hover, .btn-11:hover, .btn-12:hover {
    background-color: #f8f8f8;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f8f8f8', endColorstr='whitesmoke', GradientType='0');
    -ms-filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f8f8f8', endColorstr='whitesmoke');
    background-image: linear-gradient(to top,#f8f8f8 0,#f5f5f5 100%);
}
.btn-9, .btn-10, .btn-11, .btn-12 {
    background-color: #f7f7f7;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    display: inline-block;
    height: 18px;
    line-height: 18px;
    border: 1px solid #ddd;
    padding: 2px 14px 3px;
    color: #323333;
}
.tip-box a {
    color: #005ea7;
}
.ml10 {
    margin-left: 10px;
}
.thickwrap .thickclose:link, .thickwrap .thickclose:visited {
    display: block;
    position: absolute;
    z-index: 100000;
    top: 5px;
    right: 10px;
    overflow: hidden;
    width: 21px;
    height: 21px;
    background: url(/images/thickbox_close.png) no-repeat 0 0;
    font-size: 0;
    line-height: 100px;
}
