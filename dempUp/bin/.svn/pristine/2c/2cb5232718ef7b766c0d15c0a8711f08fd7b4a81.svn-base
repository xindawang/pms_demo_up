<table class="table">
	<tr>		
		<th>出口询价单号：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.quoteNum" readonly /> </td>
		<th>内部询价单号：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.inquiryNum" readonly /> </td>
	</tr>
	<tr>
		<th>采购方：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.aParty" readonly /></td>
		<th>采购方联系人：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.aManager" readonly /></td>		
	</tr>
	<tr>
		<th>采购方联系方式：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.aPhone" readonly /></td>
		<th>出口询价时间：</th>
		<td><input type="date" class="form-control"  name="innerInquiry.validTo" readonly /></td>
	</tr>
	<tr>
		<th>币种：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.currency" readonly/></td>
		<th>交货时间：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.deliveryAt"  readonly /></td>
	</tr>
	<tr>
		<th>交货地点：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.deliveryAddr" readonly/></td>
		<th>交货方式 ：</th>
		<td><input type="text" class="form-control"  name="innerInquiry.deliveryWay" readonly/></td>
	</tr>
	<tr>
		<th>说明：</th>
		<td colspan="3"><textarea class="form-control" name="innerInquiry.quoteDescr" style="width: 100%; height: 100px;"></textarea></td>
	</tr>	
</table>