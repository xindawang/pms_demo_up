<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
    <title>武器装备保障平台</title>
    <meta charset="UTF-8">
    <script type="text/javascript" src="/projmgt/js/common/env.js"></script>
    <script type="text/javascript" src="/iw/js/org/core.js"></script>
    <script type="text/javascript" src="/projmgt/js/common/simplePaginationEnv.js"></script>
    <script type="text/javascript" src="/projmgt/js/common/treeTable.js"></script>
    <script type="text/javascript" src="/projmgt/js/common/table.js"></script>
    <script type="text/javascript" src="/projmgt/js/common/dateTimePicker.js"></script>
    <script type="text/javascript" src="/projmgt/js/common/select.js"></script>
    <script type="text/javascript" src="/projmgt/js/plan/approval/submitDetail.js"></script>


</head>
<body>
<div id="top" class="top"></div>

<div id="left" class="left"></div>

<div class="content">
    <div class="content-header">
        <ul class="breadcrumb">
            <li><span class="glyphicon glyphicon-home"></span> <a href="/iw/org/welcome.html">首页</a></li>
            <li class="split">&gt;</li>
            <li><a href="#">项目管理</a></li>
            <li class="split">&gt;</li>
            <li class="active">计划审批</li>
        </ul>
    </div>

    <div class="content-header">
        <ul class="breadcrumb">
            <li>计划信息</li>
        </ul>
    </div>

    <form class="form-horizontal panel-body editForm" id="submitDetailForm" method="post">
        <div class="container-fluid">
            <div class="row">
                <div class="row col-md-4" style="margin-left: 0px">
                    <div class="form-group">
                        <label class="col-md-3 control-label">代号</label>
                        <div class="col-md-9 ">
                            <input id="code" name="code" placeholder="计划代号" class="form-control edit"
                                   disabled="disabled">
                        </div>
                    </div>
                    <div class=" form-group">
                        <label class="col-md-3 control-label">名称</label>
                        <div class="col-md-9 ">
                            <input id="name" name="name" placeholder="计划的名称" class="form-control edit"
                                   disabled="disabled">
                        </div>
                    </div>
                </div>

                <div class="form-group col-md-7">
                    <label class="col-md-2 control-label">说明</label>
                    <div class="col-md-10 ">
                        <textarea id="state" name="state" class="form-control edit"
                                  style="overflow-y: scroll; resize: none" rows="3" disabled="disabled"></textarea>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-1 control-label">计划开始时间</label>
                <div class="col-md-3">
                    <input id="startTime" name="startTime" class="form-control form_datetime edit" readOnly
                           disabled="disabled">
                </div>
                <label class="col-md-1 control-label">计划结束时间</label>
                <div class="col-md-3">
                    <input id="endTime" name="endTime" class="form-control form_datetime edit" readOnly
                           disabled="disabled">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">所属机构</label>
                <div class="col-md-3">
                    <input id="organization" class="form-control" disabled="disabled">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">所属型号</label>
                <div class="col-md-4">
                    <select id="type" name="type" class="form-control edit" disabled="disabled">
                        <option>--请选择--</option>
                        <option>型号1</option>
                        <option>型号2</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">所属计划</label>
                <div class="col-md-3">
                    <input id="parentName" class="form-control" disabled="disabled">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">所属任务</label>
                <div class="col-md-3">
                    <input id="superTask" class="form-control" disabled="disabled">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">参与机构</label>
                <div class="col-md-3">
                    <select id="partner" class="show-tick" disabled="disabled" multiple>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">状态</label>
                <div class="col-md-3">
                    <input id="status" name="status" class="form-control" disabled="disabled">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">优先级</label>
                <div class="col-md-3">
                    <input id="priority" name="priority" class="form-control edit" disabled="disabled">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-1 control-label">编制者</label>
                <div class="col-md-3">
                    <input id="responsible" class="form-control" disabled="disabled">
                </div>
            </div>
        </div>
    </form>

    <div class="table-box">
        <div class="content-header">
            <ul class="breadcrumb">
                <li>计划任务信息</li>
            </ul>
        </div>

        <div id="tasksTable" class="body">
            <table class="table table-hover  table-condensed">
            </table>
        </div>
    </div>


    <div class="table-box">
        <div class="content-header">
			<span style="padding-left:10px">审批意见
				<button id="acceptButton" class="btn btn-primary "><span>同意</span></button>
		 		<button id="rejectButton" class="btn btn-primary "><span>拒绝</span></button>
			</span>
        </div>


        <div class="body">
            <span>审批意见</span>
            <textarea id="statement" class="form-control" placeholder="同意"
                      style="overflow-y:scroll;width:200px;height:100px;resize: none"></textarea>
        </div>
    </div>

    <div class="table-box">
        <div class="content-header">
            <ul class="breadcrumb">
                <li>审批信息</li>
            </ul>
        </div>

        <div id="processTable" class="body">
            <table class="table table-hover  table-condensed">
            </table>
        </div>
    </div>


</div>
</body>
</html>