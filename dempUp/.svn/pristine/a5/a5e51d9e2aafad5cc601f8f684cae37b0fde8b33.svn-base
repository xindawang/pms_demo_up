@charset "utf-8";
/* CSS Document */

/* top */
.zckj_top{ min-width:1200px; height:50px; background:#333;}
.zckj_top .zckj_topCon{ width:1200px; height:50px; margin:0 auto; overflow:hidden; zoom:1;}
.zckj_top .zckj_topCon li{ float:left; height:50px; line-height:50px; display:inline; color:#fff; margin:0 18px;}
.zckj_top .zckj_topCon a{ color:#fff;}
.zckj_top .zckj_topCon .f_right li a{font-size: 14px;}
/* nav */
.zckj_nav { width:1200px; height:120px; margin: 0 auto;  zoom:1;}
.zckj_nav .zckj_log{display:block;width:340px;height:120px;background: url("../../images/main/Logo.png") 0 0 no-repeat;}
.zckj_nav .zckj_navList{height:44px; overflow:hidden; margin:60px 0 0 100px;}
.zckj_nav .zckj_navList ul li{ float:left; margin:0 10px; height:38px;display: inline;zoom: 1;}
.zckj_nav .zckj_navList ul li a{float:left;height:38px; line-height:38px; padding:0 24px; font-size:18px;color: #757575;display: inline;zoom: 1;}
.zckj_nav .zckj_navList ul li a:hover,.zckj_nav .zckj_navList ul li a.active{ background:#17afff;color:#fff;}
/* flexslider  begin */
.flexslider{position:relative;height:360px;overflow:hidden;background:url(../../images/main/loading.gif) 50% no-repeat; min-width:1200px;}
.slides{position:relative;z-index:1;}
.slides li{height:360px;}
.flex-control-nav{position:absolute;bottom:30px;z-index:2;width:100%;text-align:center;}
.flex-control-nav li{display:inline-block;width:14px;height:14px;margin:0 5px;*display:inline;zoom:1;}
.flex-control-nav a{display:inline-block;width:14px;height:14px;line-height:40px;overflow:hidden;background:#5a5959;border-radius: 50%;cursor: pointer;}
.flex-control-nav .flex-active{background: #ec7815;}
.flex-direction-nav{position:absolute;z-index:3;width:100%;top:45%;}
.flex-direction-nav li a{display:block;width:50px;height:50px;overflow:hidden;cursor:pointer;position:absolute; display:none;}
.flexslider ul.slides li a{width:100%;height:360px;display:block;}
/* flexslider  end */
/* 通告 */
.zckj_notice{min-width: 1200px;height: 48px;background: #323232;border-bottom: 1px solid #3d3d3d;padding:10px 0;}
.zckj_notice .con_w1200 div{width:550px;height: 48px;padding-left: 50px;background: url("../../images/main/zckjIndex_sprit3.png") 0 0 no-repeat;font-size: 14px;}
.zckj_notice .con_w1200 div.notice1{background-position: 0 0;}
.zckj_notice .con_w1200 div.notice2{background-position: 0 -60px;position: relative;}
.zckj_notice .con_w1200 div a{display: block;height:24px;line-height: 24px;color: #fff;}
.zckj_notice .con_w1200 div .more{position: absolute;top:24px;right:40px;color: #fff;line-height: 24px;font-size: 14px;}
/* 导航圆球 */
.zckj_navBall{ width:1200px; height:150px;margin:26px auto 18px; text-align: center;font-size: 0; overflow:hidden;}
.zckj_navBall .navBall_item{position: relative;display: inline-block;width: 350px;height: 94px;margin:4px 10px;padding:23px 0 23px 12px;border:1px solid #d6d6d6;border-radius: 4px;*display: inline;zoom: 1;font-size: 0;}
.zckj_navBall .navBall_item .navBall_bg{position: absolute;width: 44px;height: 50px;right: -4px;top: -5px;background: url('../../images/main/zckjIndexNavBallBg.png') 0 0 no-repeat;}
.zckj_navBall a.ball{float: left;width: 94px;height:94px;margin-right: 26px;background: url("../images/zckjIndexNavBall.jpg") 0 0 no-repeat;*display: inline;zoom: 1;}
.zckj_navBall a.ball1{background-position: 0 0;}
.zckj_navBall a.ball2{background-position: 0 -100px;}
.zckj_navBall a.ball3{background-position: 0 -200px;}
.zckj_navBall a.ball1:hover{background-position: -100px 0;}
.zckj_navBall a.ball2:hover{background-position: -100px -100px;}
.zckj_navBall a.ball3:hover{background-position: -100px -200px;}
.zckj_navBall  .navBall_txt{float: left;*display:inline;}
.zckj_navBall  .navBall_txt h4{height: 36px;padding-top: 14px;line-height: 36px;color: #171616;font-size: 18px;}
.zckj_navBall  .navBall_txt p{height: 30px;line-height: 30px;color: #949596;}
/* 项目中心 */
.zckj_proCenter{width: 1200px;margin: 0 auto;overflow:hidden;}
.zckj_proCenter .proCenter_tab{ height:54px; line-height: 54px;background: #17afff;overflow:hidden; zoom:1; }
.zckj_proCenter .proCenter_tab h4{float: left;height: 54px;line-height: 54px;padding-left: 24px;font-size: 32px;color: #fff;font-weight: normal;overflow: hidden;}
.zckj_proCenter .proCenter_tab h4 span{display: inline-block;line-height: 56px;margin-left: 24px;vertical-align: top;font-size: 22px;}
.zckj_proCenter .proCenter_tab .proCenter_btn{float: left;margin-left: 280px;*display: inline;}
.zckj_proCenter .proCenter_tab .proCenter_btn a{float: left;padding: 0 20px;height: 54px;line-height: 54px;color: #fbfbfb;}
.zckj_proCenter .proCenter_tab .proCenter_btn a.active{background:#fbfbfb; color: #17afff;}
.zckj_proCenter .proCenter_tab .proCenter_more{float: right;height:54px;line-height:54px;margin-right:32px;color: #fff;letter-spacing: 6px;*display: inline;}
.zckj_proCenter .proCenter_item{margin-top: 2px;padding-top: 24px;border-top: 2px solid #17afff;}
.zckj_proCenter .proCenter_item ul{position: relative;width: 1210px;*padding-bottom:28px; padding-left:4px;overflow:hidden; zoom:1; display:none;}
.zckj_proCenter .proCenter_item ul li{float:left; width:370px; height:400px;margin: 0 13px 28px 13px; border:solid 1px #dedede; background:#fff;*display:inline; }

.zckj_proCenter .proCenter_item ul li.odd{float:left; width:370px; height:440px;margin: 0 13px 28px 13px; border:solid 1px #dedede; background:#fff;*display:inline; }
.zckj_proCenter .proCenter_item ul li:hover{border-color: #17afff;}
.zckj_proCenter .proCenter_img{position: relative;}
.zckj_proCenter .proCenter_img a{display: block;width:360px; height:190px;padding:5px;}
.zckj_proCenter .proCenter_img img{display: block;width:360px; height:190px;display: block;}
.zckj_proCenter .proCenter_img .img_sign{ position:absolute; width:98px; height:27px;top:8px;right:1px;background:url(../../images/main/zckjIndex_proCenter.png) no-repeat 0 0;z-index:1;overflow:hidden;}
.zckj_proCenter .proCenter_data{ padding:0 12px; height:220px; overflow:hidden;}
.zckj_proCenter .proCenter_data h1{ height:50px; line-height:50px;font-size:16px; color:#5b5b5b; font-weight:bold;}
.zckj_proCenter .proCenter_data h1 a{color:#060606;}
.zckj_proCenter .proCenter_data p{ line-height:32px;color:#5b5b5b;}
.zckj_proCenter .proCenter_state{ height:36px; overflow:hidden; zoom:1; background:#4c4c4c; line-height:36px; color:#fff; font-size:18px;}
.zckj_proCenter .proCenter_state a{ float:left; margin:0 10px; padding-left:32px;background:url(../../images/main/zckjIndex_sprit1.png) no-repeat 0 center; color:#fff;}
.zckj_proCenter .proCenter_state .state_heart{background-position: 0 0;}
.zckj_proCenter .proCenter_state .state_follow{ background-position:0 -36px ;}
.zckj_proCenter .proCenter_state .state_comment{background-position: 0 -72px;}
/* 成员招募 */
.zckj_memCall{width: 1200px;height:400px;margin: 0 auto;}
.zckj_memCall .memCall_head{height: 54px;background: #fbc639;}
.zckj_memCall .memCall_head h4{height: 54px;line-height: 54px;padding-left: 24px;font-size: 32px;color: #fff;font-weight: normal;overflow: hidden;}
.zckj_memCall .memCall_head span{display: inline-block;line-height: 58px;margin-left: 24px;vertical-align: top;font-size: 22px;}
.zckj_memCall .memCall_item{margin-top: 2px;padding-top: 20px;border-top: 2px solid #fbc639;}
.zckj_memCall .memCall_item a{float: left;width:306px;height:292px;margin:0 46px;overflow: hidden;position: relative;}
.zckj_memCall .memCall_item img{width:306px;height:292px;}
.zckj_memCall .memCall_item img.imgb{position: absolute;top:0;left: 0;}
/* 双创资源 */
.zckj_resource{ width:1200px;margin: 0 auto;}
.zckj_resource .resource_head{height: 54px;background: #656667;}
.zckj_resource .resource_head h4{height: 54px;line-height: 54px;padding-left: 24px;font-size: 32px;color: #fff;font-weight: normal;overflow: hidden;}
.zckj_resource .resource_head span{display: inline-block;line-height: 58px;margin-left: 24px;vertical-align: top;font-size: 22px;}
.zckj_resource .resource_item{height: 198px;margin-top: 2px;border-top: 2px solid #656667;background: url("../../images/main/zckjIndex_memBg.png") 0 1px no-repeat;overflow: hidden;}
.zckj_resource .resource_item .ability{position: relative;float: left;width: 480px;height: 198px;padding-left: 65px;}
.zckj_resource .resource_item .profession{position: relative;float: right;width: 480px;height: 198px;padding-left: 65px;}
.zckj_resource .resource_item div.txt{width: 260px;padding-top: 24px;}
.zckj_resource .resource_item div.txt h5{line-height: 30px;font-size: 24px;font-weight: normal;}
.zckj_resource .resource_item div.txt h6{height: 30px;line-height: 30px;padding: 6px 0;font-size: 24px;color: #000;font-weight: normal;}
.zckj_resource .resource_item div.txt p{color: #fff;line-height: 24px;}
.zckj_resource .resource_item a.toEnter{position: absolute;width: 128px;height: 46px;top:82px;left: px;line-height: 46px;color: #fff;text-align: center;}
.zckj_resource .resource_item .ability h5{color: #17afff;}
.zckj_resource .resource_item .profession h5{color: #f15c22;}
.zckj_resource .resource_item .ability a.toEnter{right: 0;background: #f15c22;}
.zckj_resource .resource_item .profession a.toEnter{right: 34px;background: #0193de;}
/* 教学/专家 */
.zckj_teachExpert{width: 1200px;margin:0 auto;}
.zckj_teachExpert .teachExpert_tab{padding:18px 0 18px 30px;height: 34px;}
.zckj_teachExpert .teachExpert_tab a.tab{float:left;height: 32px;line-height: 32px;text-align:center;margin: 0 7px;padding: 0 28px;border:1px solid #17afff;border-radius:4px;color: #4a4a4a;font-size: 18px;}
.zckj_teachExpert .teachExpert_tab a.tab:hover,
.zckj_teachExpert .teachExpert_tab a.tab.now_tab{background :#17afff;color: #fff;}
.zckj_teachExpert .teachExpert_tab a.more{float: right;margin-right: 36px;font-size: 16px;color: #17afff;line-height: 34px;margin-top: 8px;}
.zckj_teachExpert .item_course{padding-bottom: 30px;}
.zckj_teachExpert .item_course .course_list{position: relative;width: 1150px;height: 130px;margin-top: -1px;padding: 24px;border:1px solid #fff;border-top-color:#cccfd0;overflow: hidden;z-index: 12;}
.zckj_teachExpert .item_course .course_list:hover{border-color:#17afff;z-index: 66;}
.zckj_teachExpert .item_course .course_list .img_box{float: left;width: 164px;height: 110px;padding: 10px 0;margin-right: 40px;}
.zckj_teachExpert .item_course .course_list .img_box img{width: 164px;height: 110px;}
.zckj_teachExpert .item_course .course_list .txt_box{float: left;width: 940px;height: 130px;}
.zckj_teachExpert .item_course .course_list .txt_box h4{display: inline-block;height: 30px;padding: 0 14px;margin-bottom: 10px;line-height: 30px;background: #17afff;font-size: 18px;color: #fefcf5;*display: inline;zoom: 1;}
.zckj_teachExpert .item_course .course_list .txt_box p{line-height: 24px;color: #393939;margin-bottom: 12px;}
.zckj_teachExpert .item_course .course_list .txt_box li{float: left;height: 30px;line-height: 30px;margin-right: 40px;padding-left: 44px;background: url('../../images/main/zckjIndex_teachBg.png') 0 0 no-repeat;font-size: 16px;color: #666;*display: inline;overflow: hidden;zoom: 1;}
.zckj_teachExpert .item_course .course_list .txt_box li.name{background-position: 0 0;}
.zckj_teachExpert .item_course .course_list .txt_box li.trade{background-position: 0 -31px;}
.zckj_teachExpert .item_course .course_list .txt_box li.type{background-position: 0 -60px;}
.zckj_teachExpert .item_expert{padding-top: 22px;border-top:1px solid #cccfd0;margin-top: -1px;text-align: center;font-size: 0;overflow: hidden;display: none;}
.zckj_teachExpert .item_expert .expert_list{display: inline-block;width: 540px;height: 180px;margin:0 20px 28px;padding:8px 5px;border:1px solid #dedede;*display: inline;zoom: 1;_padding-bottom:2px;}
.zckj_teachExpert .item_expert .expert_list:hover{border-color: #17afff;}
.zckj_teachExpert .item_expert .expert_list .img_box{float: left;width: 240px;height: 180px;margin-right: 30px;}
.zckj_teachExpert .item_expert .expert_list .txt_box{float: left;width: 260px;height: 166px;padding-top: 14px;text-align: left;overflow: hidden;}
.zckj_teachExpert .item_expert .expert_list .txt_box h5{height: 40px;line-height: 40px;font-size:18px;color: #686565;}
.zckj_teachExpert .item_expert .expert_list .txt_box p{color: #403c3c;font-size: 13px;line-height: 22px;}
/* 悬浮框 */
.st_tjxx{width:186px;height:202px;border:1px solid #0585af;position:fixed;right:0px;bottom:100px;background-color:#fff;z-index:9999;_position:absolute;_bottom:auto;_margin-bottom:100px;
_top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0)));}
.st-tjtitle{height:40px;color:#4cb8ff;font-size:18px;line-height:40px;text-align:center;}
.st-tj-list{background-color:#17afff;}
.st-tj-list li{border-top:1px solid #0585af;color:#fff;height:40px;line-height:40px;text-indent:1em;}
/* 公共小样式 */
.con_w1200{width: 1200px;margin: 0 auto;}
.clearfix:after{content:"";display:block;clear: both;height: 0;}
.clearfix{zoom:1;}

